{
    "version": "0.2.0",
   "configurations": [
    {
        "name": "Remote Launch & Debug",
        "type": "coreclr",
        "request": "launch",
        "program": "~/dotnet/dotnet",
        "args": ["GrowIoT.dll"],
        "preLaunchTask": "exportSync",
        "cwd": "~/GrowIoT",
        "stopAtEntry": false,
        "console": "internalConsole",
        "pipeTransport": {
            "pipeCwd": "${workspaceRoot}",
            "pipeProgram": "ssh",
            "pipeArgs": [
                "-T",
                "pi@192.168.0.153",
                "-i",
                "c:/Users/kip_o/.ssh/id_rsa"
            ],
            "debuggerPath": "~/vsdbg/vsdbg"
        }
    },
   ]
}